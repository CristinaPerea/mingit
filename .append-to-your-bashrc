source /usr/share/bash-completion/completions/git
complete -o default -F _git g
g()
{
	arg1=$1
	shift
	case "$arg1" in
		a)
			eval git add $@;;
		b)
			eval git branch $@;;
		c)
			eval git commit -m \"$1\";;
		co)
			eval git checkout $@;;
		d)
			eval git diff $@;;
		i)
			eval git init $@;;
		m)
			eval git merge $@;;
		pll)
			eval git pull $@;;
		psh)
			eval git push $@;;
		s)
			eval git status $@;;
		*)
			eval git $arg1 $@;;
	esac
}
